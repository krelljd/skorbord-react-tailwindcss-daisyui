{
  "name": "skorbord-api",
  "version": "1.0.0",
  "description": "Skorbord Card Scoring App API",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "node ./db/migrations/migrationRunner.js",
    "migrate:down": "echo 'Down migrations not supported by migrationRunner.js'",
    "migrate:create": "echo 'Create migration manually in db/migrations/'",
